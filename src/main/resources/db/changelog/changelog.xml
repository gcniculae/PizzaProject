<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.9.xsd">
    <changeSet id="1" author="George Niculae">
        <createTable tableName="test_table">
            <column name="id" type="int">
                <constraints primaryKey="true" primaryKeyName="id"/>
            </column>
            <column name="test_column" type="varchar(250)">
            </column>
        </createTable>
        <addAutoIncrement tableName="client" columnName="id" columnDataType="int" startWith="1" incrementBy="1"/>
    </changeSet>
</databaseChangeLog>
